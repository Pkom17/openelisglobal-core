<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="us.mn.state.health.lims.datasubmission.valueholder.DataIndicator" table="DATA_INDICATOR" optimistic-lock="version" dynamic-update="true">
        <id name="id" type="us.mn.state.health.lims.hibernate.resources.usertype.LIMSStringNumberUserType">
            <column name="ID" precision="10" scale="0" />
            <generator class="sequence">
              <param name="sequence">data_indicator_seq</param>
            </generator> 
        </id>
        <version name="lastupdated" column="LASTUPDATED" type="timestamp" access="field"/>
        <many-to-one name="typeOfIndicator" class="us.mn.state.health.lims.datasubmission.valueholder.TypeOfDataIndicator" fetch="select" lazy="false">
          <column name="type_of_indicator_id" precision="10" scale="0"  />
        </many-to-one>
        <property name="year" type="java.lang.Integer">
          <column name="year" />
        </property>
        <property name="month" type="java.lang.Integer">
          <column name="month" />
        </property>
        <property name="status" type="java.lang.String">
          <column name="status" />
        </property>
        <bag name="dataValues" table="data_value" lazy="false" fetch="select" cascade="save-update">
            <key> 
              <column name="indicator_id" precision="10" scale="0" />
            </key>
            <one-to-many class="us.mn.state.health.lims.datasubmission.valueholder.DataValue" />
        </bag>
        
    </class>
</hibernate-mapping>
